{
    "id" : "com.ml4w.welcome",
    "runtime" : "org.gnome.Platform",
    "runtime-version" : "47",
    "sdk" : "org.gnome.Sdk",
    "command" : "dotfiles-welcome",
    "finish-args" : [
        "--share=network",
        "--share=ipc",
        "--socket=fallback-x11",
        "--device=dri",
        "--socket=wayland",
        "--talk-name=org.freedesktop.Flatpak",
        "--filesystem=home"
    ],
    "cleanup" : [
        "/include",
        "/lib/pkgconfig",
        "/man",
        "/share/doc",
        "/share/gtk-doc",
        "/share/man",
        "/share/pkgconfig",
        "*.la",
        "*.a"
    ],
    "modules" : [
        {
            "name" : "dotfiles-welcome",
            "buildsystem" : "meson",
            "sources" : [
                {
                    "type" : "git",
                    "url" : "https://github.com/mylinuxforwork/dotfiles-welcome.git"
                }
            ],
            "config-opts" : [
                "--libdir=lib"
            ],
            "builddir" : true
        }
    ],
    "build-options" : {
        "env" : {        }
    }
}
